{
    "name": "vue-component-lib-template",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jamiefeiss/vue-component-lib-template.git"
    },
    "keywords": [
        "vue3",
        "vue",
        "vuejs"
    ],
    "files": [
        "dist"
    ],
    "main": "./dist/vue-component-lib-template.umd.cjs",
    "module": "./dist/vue-component-lib-template.js",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/vue-component-lib-template.js",
            "require": "./dist/vue-component-lib-template.umd.cjs"
        },
        "./vue-component-lib-template.css": {
            "import": "./dist/vue-component-lib-template.css",
            "require": "./dist/vue-component-lib-template.css"
        }
    },
    "types": "./dist/index.d.ts",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc && vite build",
        "preview": "vite preview",
        "test:unit": "vitest",
        "build-only": "vite build",
        "type-check": "vue-tsc --build",
        "preinstall": "npx only-allow pnpm"
    },
    "peerDependencies": {
        "vue": "^3.0.0"
    },
    "devDependencies": {
        "@types/jsdom": "^21.1.7",
        "@types/node": "^22.10.2",
        "@vitejs/plugin-vue": "^5.2.1",
        "@vue/test-utils": "^2.4.6",
        "jsdom": "^25.0.1",
        "typescript": "~5.6.3",
        "vite": "^6.0.3",
        "vite-plugin-dts": "^4.3.0",
        "vite-plugin-vue-devtools": "^7.6.8",
        "vitest": "^2.1.5",
        "vue-tsc": "^2.1.10"
    }
}